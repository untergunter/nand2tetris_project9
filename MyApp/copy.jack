class copy {

    field bool minimal;

    constructor copy new() {
        return this;
        }

    function Array copy_array(Array origen,int length){
        var Array new_copy;
        var int index;
        let new_copy = Array.new(length);
        let index=0;
        while (index<length){
            let new_copy[index] = origen[index];
            let index=index+1;
            }
        return new_copy;
        }
    }